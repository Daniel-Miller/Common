{
  "Tekton": {
    "Kernel": {
      "Database": {
        "Connection": {
          "Host": "localhost",
          "Port": 5432,
          "Database": "tekton",
          "User": "The user login name for the database connection string goes here.",
          "Password": "The password for the database connection string goes here."
        }
      },
      "Release": {
        "Environment": "Local",
        "Version": "0.0.0.0",
        "Secret": "The encryption key for this version in this environment goes here."
      },
      "Telemetry": {
        "Logging": {
          "Path": "The physical path to the Serilog log file goes here. Logging and Monitoring must have different paths."
        },
        "Monitoring": {
          "Path": "The physical path to the Sentry log file goes here. Logging and Monitoring must have different paths.",
          "Rate": 0.1,
          "Url": "The fully qualified URL for the Sentry DSN goes here."
        }
      }
    },
    "Contact": {},
    "Location": {},
    "Metadata": {},
    "Security": {
      "Domain": "example.com",
      "Token": {
        "Audience": "https://localhost:5000/api",
        "Issuer": "https://localhost:5000/api/token",
        "Lifetime": 60,
        "Whitelist": "::1, 127.0.0.1, 4.204.254.20, 20.104.26.245, 68.146.57.41"
      },
      "Permissions": [
        {
          "Type": "Http",
          "Access": "All",
          "Resources": [ "api/react" ],
          "Roles": [ "React" ]
        }
      ],
      "Sentinels": [
        {
          "Actor": {
            "Name": "React",
            "Email": "react@example.com",
            "Secret": "The secret key assigned to React sentinel goes here.",
            "Language": "en",
            "TimeZone": "Mountain Standard Time"
          },
          "Roles": [ "Reactor" ]
        },
        {
          "Actor": {
            "Name": "Root",
            "Email": "root@example.com",
            "Secret": "The secret key assigned to Root sentinel goes here."
          },
          "Roles": [ "Administrator", "Developer", "Operator" ]
        },
        {
          "Actor": {
            "Name": "Test",
            "Email": "test@example.com",
            "Secret": "The secret key assigned to Test sentinel goes here."
          },
          "Roles": [ "Tester" ]
        }
      ]
    },
    "Plugin": {
      "Integration": {
        "AstronomyApi": {
          "Host": "https://api.astronomyapi.com",
          "App": "YOUR APP ID GOES HERE",
          "Secret": "YOUR SECRET GOES HERE"
        },
        "VisualCrossing": {
          "Host": "https://weather.visualcrossing.com",
          "Secret": "YOUR SECRET GOES HERE"
        }
      }
    }
  }
}